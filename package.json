    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-react": "^7.28.5",
    "@module-federation/enhanced": "^0.23.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.3",
    "html-webpack-plugin": "^5.6.6",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
